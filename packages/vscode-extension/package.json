        },
        {
          "type": "webview",
          "id": "teamsfx-development-webview",
          "name": "Development",
          "when": "fx-extension.sidebarWelcome.treeview && fx-extension.customizedTreeview"
        },
        {
          "id": "teamsfx-deployment",
